<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bczm.com.eggapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EggActivity" />
        <activity android:name=".PacketActivity" />
        <activity android:name=".ReverseAnimationActivity" />
        <activity android:name=".UpShowActivity" />
        <activity android:name=".NewActivity" />
        <activity android:name=".SlideEggActivity"/>
    </application>

</manifest>